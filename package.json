{
  "name": "node-typescript-boilerplate",
  "version": "0.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engineStrict": true,
  "engines": {
    "node": ">= 6.9.0"
  },
  "devDependencies": {
    "@types/jest": "~20.0.4",
    "@types/node": "~6.0.85",
    "@types/request-promise": "^4.1.37",
    "@types/socket.io": "^1.4.30",
    "jest": "~20.0.0",
    "nodemon": "^1.12.0",
    "rimraf": "~2.6.1",
    "tslint": "~5.5.0",
    "tslint-microsoft-contrib": "~5.0.1",
    "tsutils": "~2.7.1",
    "typescript": "~2.4.2"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon ./build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --type-check --project 'tsconfig.json'",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "postinstall": "npm run build"
  },
  "author": "Jakub Synowiec <github@jakubsynowiec.info>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsynowiec/node-typescript-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/jsynowiec/node-typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/jsynowiec/node-typescript-boilerplate#readme",
  "dependencies": {
    "@types/moment": "^2.13.0",
    "apiai": "^4.0.3",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "firebase-admin": "^5.2.1",
    "geo-distance": "^0.1.2",
    "google-distance": "^1.0.1",
    "google-translate-api": "^2.3.0",
    "moment": "^2.18.1",
    "node-geocoder": "^3.20.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "socket.io": "^2.0.3",
    "tsc": "^1.20150623.0",
    "tslib": "~1.7.1",
    "xml2js": "^0.4.19"
  }
}
